<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="ButtonMagenta" parent="@android:style/Widget.Button">
        <item name="android:background">@color/button_magenta</item>
    </style>

    <!--style 10-->
    <style name="Walkthrough10Theme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/walkthrough10colorPrimary</item>
        <item name="colorPrimaryDark">@color/walkthrough10colorPrimaryDark</item>
        <item name="colorAccent">@color/walkthrough10colorAccent</item>
        <item name="android:windowFullscreen">true</item>
    </style>

</resources>
